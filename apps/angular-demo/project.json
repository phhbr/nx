{
  "name": "angular-demo",
  "projectType": "application",
  "sourceRoot": "src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["@designsystem/dpl-angular:build"],
      "options": {
        "command": "ng build",
        "cwd": "{projectRoot}"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ng serve",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ng test",
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ng lint",
        "cwd": "{projectRoot}"
      }
    }
  }
}
